generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Laboratoire {
  id Int @id @default(autoincrement())
  nom String @db.VarChar(200)
  logo Int[] 
  nrc Int
  active Boolean
  dateActivation DateTime? @default(now())
}
